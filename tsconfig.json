{
  "compilerOptions": {
    "outDir": "./public/",
    "sourceMap": true,
    "baseUrl": "src",
    "lib": ["es6", "dom"],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "jsx": "react",
    "checkJs": true
    // "module": "es6"
    // "target": "es5"
  },
  "include": ["./src"]
}
